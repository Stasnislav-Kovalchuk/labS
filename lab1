import math

color = "blue"
print(color)

is_voter = True
is_not_city = False

print(is_voter or is_not_city)

is_raining = True
is_snowing = True

print(not is_raining)
print(not is_snowing)

#----------------------------------------------

x = 2.14
y = 0.578

result = (math.sin(x) * (x ** 2)) + ((math.sin(x) ** 2) * (x ** 3)) + ((math.sin(x) ** 3) * (y ** 4))

print(result)
